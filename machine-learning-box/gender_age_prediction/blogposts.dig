_export:
  !include : config/params.yml
  td:
    database: ${target_db}
    engine: hive
    priority: ${job_priority}

+vectorize:
  td>: queries/blogposts/tokenize_and_ftvec.sql
  create_table: input
  engine_version: experimental

+predict:
  _parallel: true

  +rf_predict:
    call>: blogposts_rf_predict.dig
